{
  "name": "v2.routify.dev",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite",
    "build": "run-s build:*",
    "build:routify": "routify -b",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/main.js",
    "build:static": "spank",
    "build:search": "poindexter",
    "assets": "pwa-asset-generator public/logo-square.svg public/icons/pwa --background \"#FFFFFF\" --padding \"16px\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@roxi/routify": "^2.18.7",
    "@sveltech/bricks": "^1.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.44",
    "date-fns": "^2.28.0",
    "marked": "^1.2.9",
    "mdsvex": "^0.8.9",
    "npm-run-all": "^4.1.5",
    "poindexter": "^1.0.4",
    "prism-svelte": "^0.4.7",
    "prismjs": "^1.28.0",
    "pwa-asset-generator": "^6.0.8",
    "remark-slug": "^6.1.0",
    "rollup": "^2.74.1",
    "routify-plugin-frontmatter": "^1.0.1",
    "sass": "^1.52.1",
    "spank": "^1.9.0",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.6",
    "svelte-prism": "^1.1.5",
    "vite": "^2.9.9",
    "vite-main-js": "^0.0.1",
    "vite-plugin-pwa": "^0.12.0"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true,
    "plugins": {
      "routify-plugin-frontmatter": {}
    }
  },
  "configent": {
    "template": "routify-starter:1.0.0"
  },
  "poindexter": {
    "output": "dist/poindexter.bundle.json",
    "ignoreSelectors": [
      "nav",
      ".c-pagination"
    ],
    "contentSelectors": [
      ".c-sidebar-layout__main",
      "main"
    ]
  },
  "spank": {
    "entrypoint": "dist/client/index.html",
    "script": "dist/server/main.js",
    "output": "dist/server",
    "inlineDynamicImports": false
  }
}
